/* pages/list/list.wxss */
.listContent{
  display: block;
  box-sizing: border-box;
  padding: 20rpx;
  background: #ddd;
  min-height: 100vh;
}
.item{
  display: flex;
  flex-direction: row;
  height: 140rpx;
  background: #fff;
  border-radius: 10rpx;
  align-items: center;
  padding:0rpx 20rpx;
}
.item .logo{
  width:46rpx;
  height: 56rpx;
  /* flex-grow:1; */
}
.item .title{
  display: flex;
  flex-direction: column;
  flex-grow:1;/* 充满剩余空间 */
  margin-left: 20rpx;
}
.item .title .title-txt{
  font-size: 26rpx;
  margin-bottom: 26rpx;
}
.progressBlock{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.item .progressContent{
  width: 80%;
  background: #ddd;
  height: 12rpx;
  border-radius: 6rpx;
}
.item .progress-txt{
  font-size: 24rpx;
}
.item .progressContent .progress{
  background: #d2a;
  height: 12rpx;
  border-radius: 6rpx;
  width: 70%;
}